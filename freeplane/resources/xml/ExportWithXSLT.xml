<?xml version="1.0"?>
<export_with_xslt>
    <action
        name="accessories/plugins/ExportWithXSLT_HTML"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/xslt/toxhtml.xsl"
            create_dir="true"
            files_to_copy="marktree.js,minus.png,plus.png,ilink.png,treestyles.css"
            file_prefix="/html/"
            copy_icons="true"
            load_file="true"
            create_html_linked_image="false" />
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_HTML3"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/xslt/freeplane2html.xsl"
            create_dir="true"
            files_to_copy="freeplane2html.css,hide.png,leaf.png,show.png,ilink.png"
            file_prefix="/html/"
            copy_icons="true"
            load_file="true"
            create_html_linked_image="true"/>
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_Applet"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/xslt/freeplane2applet.xsl"
            create_dir="true"
            files_to_copy="freeplaneviewer.jar"
            file_prefix="$org.freeplane.core.dir.lib"
            copy_map="true"
            load_file="true" />
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_Flash"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/xslt/freeplane2flash.xsl"
            create_dir="true"
            files_to_copy="visorFreemind.swf,flashobject.js"
            file_prefix="/flash/"
            copy_map="true"
            load_file="true" />
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_TASKSTJI"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="tji"
            name_extension=".tasks"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_tji"
            xslt_file="/xslt/mm2tjitasks.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_RESOURCESTJI"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="tji"
            name_extension=".resources"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_tji"
            xslt_file="/xslt/mm2tjiresources.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
    <action
        name="accessories/plugins/ExportWithTWiki"
        location="/menu_bar/file/export/html"
    >
        <properties
            file_type="twi"
            name_extension=""
            file_description="accessories/plugins/ExportWithTWiki.properties"
            xslt_file="/xslt/mm2twiki.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
</export_with_xslt>
